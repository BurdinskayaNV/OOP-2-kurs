<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="Start" alignment="BOTTOM_CENTER" prefHeight="524.0" prefWidth="616.0" spacing="20.0" styleClass="bodybg" stylesheets="@../../../style/style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.botik.BotikController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Привет! Меня зовут Ботик!" textAlignment="CENTER">
      <font>
         <Font name="Candara Bold" size="36.0" />
      </font>
   </Text>
   <TextField fx:id="TextFieldUserName" maxHeight="50.0" maxWidth="250.0" prefHeight="50.0" prefWidth="250.0" text="Наталья">
      <font>
         <Font size="18.0" />
      </font>
   </TextField>
  <Button alignment="TOP_CENTER" onAction="#onHelloButtonClick" text="Как тебя зовут ?">
      <font>
         <Font name="Arial" size="18.0" />
      </font></Button>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
